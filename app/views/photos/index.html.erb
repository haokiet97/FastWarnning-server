<h1>Photos</h1>

<table class="responsive nowrap table table-hover" id="dttb-photos" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="id" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px">id</th>
      <th data-data="title" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px">title</th>
      <th data-data="created_at" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px">created at</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px">more</th>
    </tr>
  </thead>

</table>

<br>

<div id="photos">
  <%= render partial: "photos", locals: { photos: @photos, camera: @camera } %>
</div>

<%= link_to 'New Photo', new_photo_path %>
