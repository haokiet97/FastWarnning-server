<h1>Videos</h1>

<table class="responsive nowrap table table-hover" id="dttb-videos" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="title">Title</th>
      <th data-data="description">Description</th>
      <th data-data="created_at">Created at</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px">Actions</th>
    </tr>
  </thead>

</table>

<br>
<div id="videos">
  <%= render partial: "videos", locals: { videos:@videos, camera: @camera } %>
</div>
<%= link_to 'New Video', new_video_path, class: "btn btn-primary" %>
